{
  "permissions": {
    "allow": [
      "Bash(npm install)",
      "Bash(npm run *)",
      "Bash(npx *)",
      "Bash(git *)",
      "Bash(ls *)",
      "Bash(mkdir *)",
      "Bash(cp *)",
      "Bash(bash *)",
      "mcp__atlassian__*",
      "mcp__github__*",
      "mcp__gutt-mcp-remote__*"
    ],
    "deny": []
  },
  "mcpServers": {},
  "hooks": {
    "SessionStart": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node hooks/sessionstart-setup.cjs"
          }
        ]
      }
    ]
  }
}
